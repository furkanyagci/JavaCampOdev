package staticDemo;

public class ProductValidator {
	
	public static boolean isValid(Product product) {//Static uygulama duruncaya kadar bellekte durur. new lersek iþlem bitince bellekten atýlýr
		if (product.price>0 && !product.name.isEmpty()) {
			return true;
		}else {
			return false;
		}
	}
	
}
